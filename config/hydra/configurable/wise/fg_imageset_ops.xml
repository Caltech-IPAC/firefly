<!DOCTYPE FieldGroup SYSTEM "../../project.dtd">

<FieldGroup xid="imageset" labelWidth="65" height="25">
    <EnumString>
        <ID>pass2_schema</ID>
        <Name>schema</Name>
        <Title>Image Set:</Title>
        <PreferWidth>150</PreferWidth>
        <Mask>[CHECKBOX]</Mask>
        <Default>allsky-4band</Default>
        <Pref-Key>wise.schema</Pref-Key>
        <ShortDescription>Image set to query</ShortDescription>
	<ErrorDescription>At least one image set must be selected</ErrorDescription>
        <EnumValue id="allwise">
            <Title>AllWISE (multi-band)</Title>
        </EnumValue>
        <EnumValue id="allsky-4band">
            <Title>All-Sky (4 band)</Title>
        </EnumValue>
        <EnumValue id="cryo_3band">
            <Title>3-Band Cryo</Title>
        </EnumValue>
        <EnumValue id="postcryo">
            <Title>Post-Cryo (2 band)</Title>
        </EnumValue>
    </EnumString>
</FieldGroup>
