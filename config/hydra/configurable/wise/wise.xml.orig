<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Project SYSTEM "../../../configurable/project.dtd">

<Project>
    <Name>wise</Name>
    <Title>Wide-Field Infrared Survey Explorer</Title>
    <Properties>
        <Param key="footer" value="irsa_footer.html"/>
        <Param key="smallIcon" value="wise_logo_left.html"/>
        <Param key="helpMenu" value="WISE Help;WISE Help;overviewHelp"/>
        <Param key="about" value="About WISE;About WISE;http://irsa.ipac.caltech.edu/Missions/wise.html"/>
    </Properties>
    <Catalog name="WISEPASS1">
        <Host>http://***REMOVED***</Host>
        <Credentials login="wise" password="orare{wise}"/>
        <CatalogURL><![CDATA[/cgi-bin/Gator/nph-scan?proj=WISEPASS1&WISEPASS1=orare{wise}&mode=ascii]]></CatalogURL>
        <OriginalFilename>master-wise-cat-original.tbl</OriginalFilename>
        <MasterCatFilename>master-wise-catalog.tbl</MasterCatFilename>
        <AddtlReqSearchParams>
            <GatorParam requestName="server" masterCatName="server"/>
            <GatorParam requestName="database" masterCatName="database"/>
            <GatorParam requestName="ddfile" masterCatName="ddname"/>
            <GatorParam requestName="xpffile" masterCatName="xpffile"/>
        </AddtlReqSearchParams>
    </Catalog>
    <SearchType helpId="HelpMenu.wise.searching.byPosition">
        <Name>wise_1</Name>
        <Title>Position</Title>
        <Tooltip>Region Search</Tooltip>
        <Form>
            <FormEventWorker id="fdvController_1" type="FieldDefVisibilityControl" xid="fdvController_1">
                <Param key="masters" value="intersect=CENTER"></Param>
                <Param key="show" value="subsize,mcenter"></Param>
                <Param key="hide" value="size"></Param>
            </FormEventWorker>
            <FormEventWorker id="fdvController_2" type="FieldDefVisibilityControl" xid="fdvController_2">
                <Param key="masters" value="or(intersect=ENCLOSED,intersect=OVERLAPS)"></Param>
                <Param key="show" value="size"></Param>
                <Param key="hide" value="subsize,mcenter"></Param>
            </FormEventWorker>
            <FormEventWorker id="fdvController_3" type="FieldDefVisibilityControl" xid="fdvController_3">
                <Param key="masters" value="intersect=COVERS"></Param>
                <Param key="show" value="size,subsize,mcenter"></Param>
            </FormEventWorker>
            <FormEventWorker id="cpvController_1" type="CollapsiblePanelVisibilityControl">
                <Param key="masters" value="optLevel=1b"></Param>
                <Param key="slaves" value="L1"></Param>
            </FormEventWorker>
            <FormEventWorker id="cpvController_2" type="CollapsiblePanelVisibilityControl">
                <Param key="masters" value="or(optLevel=3a,optLevel=3o)"></Param>
                <Param key="slaves" value="L3"></Param>
            </FormEventWorker>
            <FormEventWorker id="fdvController_4" type="FieldDefVisibilityControl" xid="fdvController_4">
                <Param key="masters" value="frameOp=be"></Param>
                <Param key="show" value="frameVal1,frameVal2"></Param>
                <Param key="hide" value="frameVal3"></Param>
                <Param key="retainSpace" value="false"></Param>
            </FormEventWorker>
            <FormEventWorker id="fdvController_5" type="FieldDefVisibilityControl" xid="fdvController_5">
                <Param key="masters" value="frameOp=in"></Param>
                <Param key="show" value="frameVal3"></Param>
                <Param key="hide" value="frameVal1,frameVal2"></Param>
                <Param key="retainSpace" value="false"></Param>
            </FormEventWorker>
            <FormEventWorker id="fdvController_6" type="FieldDefVisibilityControl" xid="fdvController_6">
                <Param key="masters" value="or(frameOp=eq,frameOp=gt,frameOp=lt)"></Param>
                <Param key="show" value="frameVal1"></Param>
                <Param key="hide" value="frameVal2,frameVal3"></Param>
                <Param key="retainSpace" value="false"></Param>
            </FormEventWorker>
            <FieldGroup align="left" labelWidth="300">
                <FieldGroup type="activeTabPane" typeName="POS UPL TabPane" width="640px" height="106px">
                    <FieldGroup>
                        <Title>Single Object</Title>
                        <PreDefField id="simpleTargetPanel"/>
                    </FieldGroup>
                    <FieldGroup labelWidth="70">
                        <Title>Multi-Objects</Title>
                        <File>
                            <Name>filename</Name>
                            <Title>File Name</Title>
                            <Size>62</Size>
                            <ShortDescription>Select a file to upload</ShortDescription>
                            <ErrorDescription>File upload error</ErrorDescription>
                            <Cached/>
                        </File>
                        <Help helpId="HelpMenu.wise.searching.byTable">
                            <Title>Help on file format</Title>
                        </Help>
                    </FieldGroup>
                </FieldGroup>
                <Label><![CDATA[&nbsp;]]></Label>
                <FieldGroup align="left" labelWidth="300" xid="regionSearch">
                    <EnumString>
                        <Name>intersect</Name>
                        <Title>Search Type (Region Intersection)</Title>
                        <PreferWidth>150</PreferWidth>
                        <Default>CENTER</Default>
                        <ShortDescription>Search Type (Region Intersection)</ShortDescription>
                        <EnumValue id="CENTER">
                            <Value>CENTER</Value>
                            <Title>Image contains target</Title>
                        </EnumValue>
                        <EnumValue id="COVERS">
                            <Value>COVERS</Value>
                            <Title>Image covers entire search region</Title>
                        </EnumValue>
                        <EnumValue id="ENCLOSED">
                            <Value>ENCLOSED</Value>
                            <Title>Image is entirely enclosed by search region</Title>
                        </EnumValue>
                        <EnumValue id="OVERLAPS">
                            <Value>OVERLAPS</Value>
                            <Title>Any pixel overlaps search region</Title>
                        </EnumValue>
                    </EnumString>
                    <DEGREE units="arcsec">
                        <Name>size</Name>
                        <Title>Search Region (Square) Size</Title>
                        <Default>30</Default>
                        <PreferWidth>16</PreferWidth>
                        <ShortDescription>Enter the region size of the search</ShortDescription>
                        <ErrorDescription>Region size out of range</ErrorDescription>
                        <Min>1</Min>
                        <Max>43200</Max>
                        <Cached/>
                    </DEGREE>
                    <DEGREE units="arcsec">
                        <Name>subsize</Name>
                        <Title>Return Image Size (leave blank for full images)</Title>
                        <Default>600</Default>
                        <PreferWidth>16</PreferWidth>
                        <ShortDescription>Specifies the size of the desired L1b or Atlas image cutout. Omitting this constraint will result in visualization/return of entire L1b/Atlas images.

In multi-input mode, this takes effect only for input tables without a subsize column.</ShortDescription>
                        <ErrorDescription>Invalid entry!</ErrorDescription>
                        <Min>1</Min>
                        <Max>7200</Max>
                        <NullAllowed/>
                    </DEGREE>
                    <EnumString>
                        <Name>mcenter</Name>
                        <Title>Return only the most centered image containing the target:</Title>
                        <PreferWidth>150</PreferWidth>
                        <Mask>[RADIO]</Mask>
                        <Default>all</Default>
                        <ShortDescription>Specifies whether to return only the most centered (in pixel space) image-set for the given input position.

In multi-input mode, this flag applies only for input tables without a best column.</ShortDescription>
                        <NullAllowed/>
                        <EnumValue id="all">
                            <Value>all</Value>
                            <Title>No</Title>
                        </EnumValue>
                        <EnumValue id="mcen">
                            <Value>mcen</Value>
                            <Title>Yes</Title>
                        </EnumValue>
                    </EnumString>
                    <FieldGroup xid="bandSelection" labelWidth="140">
                        <EnumString>
                            <Name>band</Name>
                            <Title>Return the following bands:</Title>
                            <ShortDescription>Band Selection: W1=3.4μm, W2=4.6μm, W3=12μm, W4=22μm</ShortDescription>
                            <ErrorDescription>Please select at least one band.</ErrorDescription>
                            <Mask>[CHECKBOX]</Mask>
                            <Default>_all_</Default>
                            <EnumValue id="1">
                                <Value>1</Value>
                                <Title>W1</Title>
                            </EnumValue>
                            <EnumValue id="2">
                                <Value>2</Value>
                                <Title>W2</Title>
                            </EnumValue>
                            <EnumValue id="3">
                                <Value>3</Value>
                                <Title>W3</Title>
                            </EnumValue>
                            <EnumValue id="4">
                                <Value>4</Value>
                                <Title>W4</Title>
                            </EnumValue>
                        </EnumString>
                    </FieldGroup>
                    <FieldGroup xid="optLevelRestricted" labelWidth="140">
                        <Access includes="WISE:ALL"/>
                        <EnumString orientation="horizontal">
                            <Name>optLevel</Name>
                            <Title>Data Product Level:</Title>
                            <Mask>[CHECKBOX]</Mask>
                            <PreferWidth>225</PreferWidth>
                            <Default>3a</Default>
                            <ShortDescription>Data Product Level</ShortDescription>
                            <ErrorDescription>Select at least one product!</ErrorDescription>
                            <EnumValue id="1b">
                                <Value>1b</Value>
                                <Title>Single Exposure (Level 1b)</Title>
                            </EnumValue>
                            <EnumValue id="3o">
                                <Value>3o</Value>
                                <Title>Level 3o</Title>
                            </EnumValue>
                            <EnumValue id="3a">
                                <Value>3a</Value>
                                <Title>Atlas (Level 3)</Title>
                            </EnumValue>
                        </EnumString>
                    </FieldGroup>
                    <FieldGroup xid="optLevelPublic" labelWidth="140">
                        <Access excludes="WISE:ALL"/>
                        <EnumString orientation="horizontal">
                            <Name>optLevel</Name>
                            <Title>Data Product Level:</Title>
                            <Mask>[CHECKBOX]</Mask>
                            <PreferWidth>225</PreferWidth>
                            <Default>3a</Default>
                            <ShortDescription>Data Product Level</ShortDescription>
                            <ErrorDescription>Select at least one product!</ErrorDescription>
                            <EnumValue id="1b">
                                <Value>1b</Value>
                                <Title>Single Exposure (Level 1b)</Title>
                            </EnumValue>
                            <EnumValue id="3a">
                                <Value>3a</Value>
                                <Title>Atlas (Level 3)</Title>
                            </EnumValue>
                        </EnumString>
                    </FieldGroup>
                    <FieldGroup type="activeCollapsiblePanel" typeName="L1" labelWidth="200">
                        <Title>Optional constraints for Single Exposure (Level 1b) data</Title>
                        <FieldGroup labelWidth="580">
                            <FieldGroup direction="horizontal" labelWidth="130">
                                <Date>
                                    <Name>timeStart</Name>
                                    <Title>Start Date (UT)</Title>
                                    <ShortDescription>Period start (yyyy-MM-dd [HH:mm:ss])</ShortDescription>
                                    <ErrorDescription>Format error!</ErrorDescription>
                                    <Format>yyyy-MM-dd;yyyy-MM-dd HH:mm:ss</Format>
                                    <NullAllowed/>
                                </Date>
                                <Date>
                                    <Name>timeEnd</Name>
                                    <Title>End Date (UT)</Title>
                                    <ShortDescription>Period end (yyyy-MM-dd [HH:mm:ss])</ShortDescription>
                                    <ErrorDescription>Format error!</ErrorDescription>
                                    <Format>yyyy-MM-dd;yyyy-MM-dd HH:mm:ss</Format>
                                    <NullAllowed/>
                                </Date>
                            </FieldGroup>
                            <Label>
                                <![CDATA[<i>&nbsp;&nbsp;&nbsp;&nbsp;Enter date range to search, format example: </i>2010-01-10 15:30:00<i>, or </i>2010-01-10<i>.</i><br><br>]]></Label>
                        </FieldGroup>
                        <FieldGroup align="left" labelWidth="144">
                            <String>
                                <Name>scanId</Name>
                                <Title>Scan ID(s)</Title>
                                <PreferWidth>50</PreferWidth>
                                <ShortDescription>Optionally provide a WISE scan identifier consisting of up to 5 digits and a letter (a, b, c, d, e, or x). Together with an in-scan frame number, this uniquely identifies a single epoch WISE image set. Note that leading zeroes are insignificant and may be omitted.

In multi-input mode, this constraint applies only if the input table does not have a scan column.</ShortDescription>
                                <NullAllowed/>
                            </String>
                            <FieldGroup direction="horizontal" labelWidth="144">
                                <EnumString orientation="vertical">
                                    <Name>frameOp</Name>
                                    <Title>Frame Number(s) (optional)</Title>
                                    <Default>eq</Default>
                                    <ShortDescription>A WISE L1b image frame number or an acceptable frame number range may be provided.

In multi-input mode, these constraints apply only if the input table does not have a frame_min or frame_max column.

Frame numbers are non-negative integers uniquely identifying an image set within a specific scan.</ShortDescription>
                                    <NullAllowed/>
                                    <EnumValue id="eq">
                                        <Value>eq</Value>
                                        <Title>=</Title>
                                    </EnumValue>
                                    <EnumValue id="gt">
                                        <Value>gt</Value>
                                        <Title>&gt;</Title>
                                    </EnumValue>
                                    <EnumValue id="lt">
                                        <Value>lt</Value>
                                        <Title>&lt;</Title>
                                    </EnumValue>
                                    <EnumValue id="in">
                                        <Value>in</Value>
                                        <Title>IN</Title>
                                    </EnumValue>
                                    <EnumValue id="be">
                                        <Value>be</Value>
                                        <Title>BETWEEN</Title>
                                    </EnumValue>
                                </EnumString>
                                <FieldGroup align="left" direction="horizontal" labelWidth="1">
                                    <Label><![CDATA[<div style='width:18px'>&nbsp;</div]]></Label>
                                    <String>
                                        <Name>frameVal1</Name>
                                        <Title></Title>
                                        <PreferWidth>10</PreferWidth>
                                        <ShortDescription>Enter frame number</ShortDescription>
                                        <NullAllowed/>
                                    </String>
                                    <String>
                                        <Name>frameVal2</Name>
                                        <Title></Title>
                                        <PreferWidth>10</PreferWidth>
                                        <ShortDescription>Enter end frame number</ShortDescription>
                                        <NullAllowed/>
                                    </String>
                                    <String>
                                        <Name>frameVal3</Name>
                                        <Title></Title>
                                        <PreferWidth>30</PreferWidth>
                                        <ShortDescription>Enter list of frame numbers</ShortDescription>
                                        <NullAllowed/>
                                    </String>
                                </FieldGroup>
                            </FieldGroup>
                        </FieldGroup>
                    </FieldGroup>
                    <FieldGroup type="activeCollapsiblePanel" typeName="L3" labelWidth="200">
                        <Title>Optional constraints for Atlas (Level 3) data</Title>
                        <FieldGroup labelWidth="144">
                            <String>
                                <Name>coaddId</Name>
                                <Title>Coadd ID(s)</Title>
                                <PreferWidth>50</PreferWidth>
                                <ShortDescription>Coadd ID(s)</ShortDescription>
                                <NullAllowed/>
                            </String>
                        </FieldGroup>
                    </FieldGroup>
                </FieldGroup>
            </FieldGroup>
        </Form>
        <Query id="wiseQuery_1b" searchProcessorId="WiseQuery" xid="wiseQuery_1b">
            <Constraints>
                <And>
                    <Condition fieldDefHidden="filename"/>
                    <Condition fieldDefId="optLevel" value="1b"/>
                </And>
            </Constraints>
            <Param key="host" value="%wise.ibe.host%"/>
            <Param key="schemaGroup" value="wise"/>
            <Param key="schema" value="pass1"/>
            <Param key="table" value="i1bm_frm"/>
            <Param key="ProductLevel" value="1b"/>
            <Param key="SourceTable" value="i1bs_psd"/>
            <Param key="SortInfo" value="SortInfo=ASC,scan_id,frame_num,band"/>
            <Metadata>
                <Param key="host" value="%wise.ibe.host%"/>
                <Param key="schemaGroup" value="wise"/>
                <Param key="schema" value="pass1"/>
                <Param key="table" value="i1bm_frm"/>
                <Param key="ProductLevel" value="1b"/>
                <Param key="ALL_CORNERS"
                       value="ra1;dec1;EQ_J2000,ra2;dec2;EQ_J2000,ra3;dec3;EQ_J2000,ra4;dec4;EQ_J2000"/>
                <Param key="CENTER_COLUMN" value="crval1;crval2;EQ_J2000"/>
                <Param key="PREVIEW_COLUMN" value="download"/>
                <Param key="RESOURCE_TYPE" value="URL"/>
                <Param key="col.related" value="scan_id,frame_num"/>
                <Param key="col.in_row_id.Visibility" value="hide"/>
                <Param key="col.in_ra.Visibility" value="hide"/>
                <Param key="col.in_dec.Visibility" value="hide"/>
                <Param key="col.crval1.Visibility" value="hide"/>
                <Param key="col.crval2.Visibility" value="hide"/>
                <Param key="col.ra1.Visibility" value="hide"/>
                <Param key="col.ra2.Visibility" value="hide"/>
                <Param key="col.ra3.Visibility" value="hide"/>
                <Param key="col.ra4.Visibility" value="hide"/>
                <Param key="col.dec1.Visibility" value="hide"/>
                <Param key="col.dec2.Visibility" value="hide"/>
                <Param key="col.dec3.Visibility" value="hide"/>
                <Param key="col.dec4.Visibility" value="hide"/>
                <Param key="col.scan_id.SortByCols" value="scan_id,frame_num,band"/>
                <Param key="col.frame_num.SortByCols" value="frame_num,scan_id,band"/>
            </Metadata>
            <Download id="WiseDownload" title="Download Options" filePrefix="WISE_Files" titlePrefix="WISE" maxRows="5000">
                <Param key="host" value="%wise.ibe.host%"/>
                <Param key="schemaGroup" value="wise"/>
                <Param key="schema" value="pass1"/>
                <Param key="table" value="i1bm_frm"/>
                <Param key="ProductLevel" value="1b"/>
                <SearchFormParam keyName="subsize" keyValue="*" createParams="subsize,UserTargetWorldPt"/>
                <FieldGroup labelWidth="130">
                    <EnumString orientation="horizontal">
                        <Name>allBands</Name>
                        <Title>Download All Bands (1-4):</Title>
                        <Mask>[RADIO]</Mask>
                        <PreferWidth>400</PreferWidth>
                        <Default>yes</Default>
                        <ShortDescription>Download All Bands</ShortDescription>
                        <EnumValue id="yes">
                            <Value>yes</Value>
                            <Title>Yes</Title>
                        </EnumValue>
                        <EnumValue id="no">
                            <Value>no</Value>
                            <Title>No</Title>
                        </EnumValue>
                    </EnumString>
                    <EnumString orientation="horizontal">
                        <Name>artFiles</Name>
                        <Title>Download Ancillary Files:</Title>
                        <Mask>[CHECKBOX]</Mask>
                        <PreferWidth>400</PreferWidth>
                        <Default>_none_</Default>
                        <ShortDescription>Download Ancillary Files</ShortDescription>
                        <NullAllowed/>
                        <EnumValue id="M">
                            <Value>M</Value>
                            <Title>Mask</Title>
                        </EnumValue>
                        <EnumValue id="U">
                            <Value>U</Value>
                            <Title>Uncertainty</Title>
                        </EnumValue>
                        <EnumValue id="D">
                            <Value>D</Value>
                            <Title>Diffraction Spikes/Halos</Title>
                        </EnumValue>
                        <EnumValue id="O">
                            <Value>O</Value>
                            <Title>Optical Ghosts</Title>
                        </EnumValue>
                        <EnumValue id="P">
                            <Value>P</Value>
                            <Title>Latents</Title>
                        </EnumValue>
                    </EnumString>
                    <FieldGroup labelWidth="130" downloadRestriction="subsize=*">
                        <EnumString orientation="vertical">
                            <Name>dlCutouts</Name>
                            <Title>Download</Title>
                            <PreferWidth>400</PreferWidth>
                            <Default>cut</Default>
                            <ShortDescription>Download Cutouts Option</ShortDescription>
                            <EnumValue id="cut">
                                <Value>cut</Value>
                                <Title>Specified Cutouts</Title>
                            </EnumValue>
                            <EnumValue id="orig">
                                <Value>orig</Value>
                                <Title>Original Images</Title>
                            </EnumValue>
                        </EnumString>
                    </FieldGroup>
                    <EnumString orientation="vertical">
                        <Name>zipType</Name>
                        <Title>Zip File Structure</Title>
                        <PreferWidth>400</PreferWidth>
                        <Default>folder</Default>
                        <ShortDescription>Zip File Structure</ShortDescription>
                        <EnumValue id="folder">
                            <Value>folder</Value>
                            <Title>Structured (with folders)</Title>
                        </EnumValue>
                        <EnumValue id="flat">
                            <Value>flat</Value>
                            <Title>Flattened (no folders)</Title>
                        </EnumValue>
                    </EnumString>
                    <Label><![CDATA[&nbsp;]]></Label>
                </FieldGroup>
            </Download>
        </Query>
