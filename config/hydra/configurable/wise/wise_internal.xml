<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Project SYSTEM "../../project.dtd" [
        <!ENTITY few_intersect_visi_cntrl      SYSTEM "few_intersect_visi_cntrl.xml.txt">
        <!ENTITY few_dplvl_panel_visi_cntrl   SYSTEM "few_dplvl_panel_visi_cntrl.xml.txt">
        <!ENTITY few_frameop_visi_cntrl        SYSTEM "few_frameop_visi_cntrl.xml.txt">
        <!ENTITY few_scanid_cntrl              SYSTEM "few_scanid_cntrl.xml.txt">
        <!ENTITY few_source_tabpane_cntrl      SYSTEM "few_source_tabpane_cntrl.xml.txt">
        <!ENTITY few_source_dplvl_visi_cntrl  SYSTEM "few_source_dplvl_visi_cntrl.xml.txt">
        <!ENTITY few_source_tabpane_resize     SYSTEM "few_source_tabpane_resize.xml.txt">
        <!ENTITY few_mos_visi_cntrl            SYSTEM "few_mos_visi_cntrl.xml.txt">
        <!ENTITY few_imageset_visi_cntrl       SYSTEM "few_imageset_visi_cntrl.xml.txt">
        ]>

<Project>
    <Name>wise</Name>
    <Title>WISE</Title>

    <xi:include href="prp_wise.xml"/>

    <Catalog name="WISEPASS2">
        <Host>https://wise:orare{wise}@ceres.ipac.caltech.edu</Host>
        <CatalogURL><![CDATA[/cgi-bin/Gator/nph-scan?proj=WISEPASS2&WISEPASS2=orare{wise}&mode=ascii&dbms=20]]></CatalogURL>
        <OriginalFilename>master-wise-cat2-original.tbl</OriginalFilename>
        <MasterCatFilename>master-wise-catalog.tbl</MasterCatFilename>
        <AddtlReqSearchParams>
            <GatorParam requestName="server" masterCatName="server"/>
            <GatorParam requestName="database" masterCatName="database"/>
            <GatorParam requestName="ddfile" masterCatName="ddname"/>
            <GatorParam requestName="xpffile" masterCatName="xpffile"/>
        </AddtlReqSearchParams>
    </Catalog>
    <Catalog name="WISEPASS1">
        <Host>https://wise:orare{wise}@ceres.ipac.caltech.edu</Host>
        <CatalogURL><![CDATA[/cgi-bin/Gator/nph-scan?proj=WISEPASS1&WISEPASS1=orare{wise}&mode=ascii&dbms=20]]></CatalogURL>
        <OriginalFilename>master-wise-cat1-original.tbl</OriginalFilename>
        <MasterCatFilename>master-wise-catalog.tbl</MasterCatFilename>
        <AddtlReqSearchParams>
            <GatorParam requestName="server" masterCatName="server"/>
            <GatorParam requestName="database" masterCatName="database"/>
            <GatorParam requestName="ddfile" masterCatName="ddname"/>
            <GatorParam requestName="xpffile" masterCatName="xpffile"/>
        </AddtlReqSearchParams>
    </Catalog>
    <SearchGroup>
        <Name>general</Name>
        <Title>General</Title>

        <SearchType layoutSelector="true" helpId="searching.byPosition">
            <Name>wise_1</Name>
            <Title>Position</Title>
            <Tooltip>Region Search</Tooltip>
            <Form>
                &few_imageset_visi_cntrl;
                &few_intersect_visi_cntrl;
                &few_dplvl_panel_visi_cntrl;
                &few_frameop_visi_cntrl;

                <xi:include href="fg_imageset_allwisetest.xml"/>

                <FieldGroup align="left" labelWidth="300" width="850px">

                    <xi:include href="fg_pos_upload.xml"/>

                    <Label><![CDATA[&nbsp;]]></Label>
                    <FieldGroup align="left" labelWidth="300" xid="regionSearch">
                        <xi:include href="f_intersect.xml"/>
                        <xi:include href="f_size.xml"/>
                        <xi:include href="f_subsize.xml"/>
                        <xi:include href="f_mcenter.xml"/>
                        <xi:include href="fg_band_sel.xml"/>
                        <xi:include href="fg_3band_sel.xml"/>
                        <xi:include href="fg_2band_sel.xml"/>
                        <xi:include href="fg_dplvl.xml"/>
                        <xi:include href="fg_dplvl_pass1.xml"/>
                        <xi:include href="fg_dplvl_1b.xml"/>
                        <xi:include href="fg_dplvl_3a.xml"/>
                        <xi:include href="fg_L1_addtl_opts.xml"/>
                        <xi:include href="fg_L3_addtl_opts.xml"/>
                    </FieldGroup>
                </FieldGroup>
            </Form>
<!-- query section -->
            <xi:include href="qry_1b.xml"/>
            <xi:include href="qry_1b_upload.xml"/>

            <xi:include href="qry_3a.xml"/>
            <xi:include href="qry_3a_upload.xml"/>

            <xi:include href="qry_3o.xml"/>
            <xi:include href="qry_3o_upload.xml"/>
<!-- result section -->
            <xi:include href="res_wise.xml"/>

        </SearchType>

        <SearchType layoutSelector="true" helpId="searching.bySolarObject">
            <Name>wise_5</Name>
            <Title>Solar System Object/Orbit</Title>
            <Tooltip>Solar System Object/Orbit</Tooltip>

            <Form>
                &few_mos_visi_cntrl;
                &few_imageset_visi_cntrl;
                <xi:include href="fg_imageset.xml"/>
                <xi:include href="fg_mos_tabpane.xml"/>
                <FieldGroup align="left" labelWidth="240" width="850px">
                    <xi:include href="f_subsize.xml"/>
                    <xi:include href="fg_band_sel.xml"/>
                    <xi:include href="fg_3band_sel.xml"/>
                    <xi:include href="fg_2band_sel.xml"/>
                </FieldGroup>
            </Form>

<!-- query section -->
            <xi:include href="qry_1b_mos.xml"/>
<!-- result section -->
            <xi:include href="res_wise_mos.xml"/>

        </SearchType>
    </SearchGroup>

    <SearchGroup>
        <Name>advanced</Name>
        <Title>Advanced</Title>

        <SearchType layoutSelector="true" helpId="searching.byScanFrame">
            <Name>wise_2</Name>
            <Title>Scan ID/Frame (Single Exposure)</Title>
            <Tooltip>Scan ID/Frame (Single Exposure)</Tooltip>
            <Form>
                &few_frameop_visi_cntrl;
                &few_imageset_visi_cntrl;
                &few_scanid_cntrl;
                <FieldGroup align="left" labelWidth="144"  width="625px">
                    <xi:include href="fg_imageset.xml"/>
                    <xi:include href="f_scan_id.xml"/>
                    <xi:include href="fg_frame_op.xml"/>
                    <xi:include href="fg_band_sel.xml"/>
                    <xi:include href="fg_3band_sel.xml"/>
                    <xi:include href="fg_2band_sel.xml"/>
                    <HIDDEN>
                        <ID>dtLevel_single</ID>
                        <Name>dpLevel</Name>
                        <Default>1b</Default>
                    </HIDDEN>
                </FieldGroup>
            </Form>

            <Query id="wiseQuery_1b" searchProcessorId="WiseQuery" ref-xid="wiseQuery_1b">
                <Constraints>
                <Condition fieldDefId="dpLevel" value="1b"/>
                </Constraints>
            </Query>

            <xi:include href="res_wise.xml"/>

        </SearchType>

        <SearchType layoutSelector="true" helpId="searching.byCoadd">
            <Name>wise_3</Name>
            <Title>Coadd ID (Atlas)</Title>
            <Tooltip>Coadd ID (Atlas)</Tooltip>
            <Form>
                &few_imageset_visi_cntrl;
                <FieldGroup align="left" labelWidth="144" width="625px">
                    <xi:include href="fg_imageset_coadd.xml"/>
                    <xi:include href="fg_coadd_id.xml"/>
                    <xi:include href="fg_band_sel.xml"/>
                    <xi:include href="fg_3band_sel.xml"/>
                    <xi:include href="fg_2band_sel.xml"/>
                    <FieldGroup labelWidth="140">
                        <xi:include href="fg_dplvl_coadd_pass1.xml"/>
                        <xi:include href="fg_dplvl_coadd.xml"/>
                    </FieldGroup>
                </FieldGroup>
            </Form>

            <xi:include href="qry_3a.xml"/>
            <xi:include href="qry_3o.xml"/>

            <xi:include href="res_wise.xml"/>

        </SearchType>

        <SearchType layoutSelector="true" helpId="searching.bySource">
            <Name>wise_4</Name>
            <Title>WISE Source ID</Title>
            <Tooltip>WISE Source ID</Tooltip>
            <Form>
                &few_imageset_visi_cntrl;
                &few_intersect_visi_cntrl;
                &few_source_dplvl_visi_cntrl;
                &few_source_tabpane_cntrl;
                &few_source_tabpane_resize;

                <xi:include href="fg_imageset.xml"/>
                <xi:include href="fg_source_tab_internal.xml"/>

                <FieldGroup labelWidth="240" width="850px">
                    <xi:include href="fg_band_sel.xml"/>
                    <xi:include href="fg_3band_sel.xml"/>
                    <xi:include href="fg_2band_sel.xml"/>
                    <xi:include href="fg_dplvl_pass1.xml"/>
                    <xi:include href="fg_dplvl_1b.xml"/>
                    <xi:include href="fg_dplvl.xml"/>
                </FieldGroup>
            </Form>

            <xi:include href="qry_1b.xml"/>
            <xi:include href="qry_3a.xml"/>
            <xi:include href="qry_3o.xml"/>

            <xi:include href="res_wise.xml"/>

        </SearchType>

    </SearchGroup>

</Project>
