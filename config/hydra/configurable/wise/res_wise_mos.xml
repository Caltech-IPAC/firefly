                <xi:include href="pv_band2_mos.xml"/>
                <xi:include href="pv_band3_mos.xml"/>
                <xi:include href="pv_band4_mos.xml"/>
            </South>

            <West layout="fixedTab" initialWidth="330">
                <xi:include href="pv_coverage_mos.xml"/>
                <xi:include href="pv_3color_mos.xml"/>
                <xi:include href="pv_details.xml"/>
                <Preview type="DataSourceCoverageView">
                    <QueryId>wiseMOSQuery_1b</QueryId>
                    <EventWorkerId>movingTrackMOSPath,movingTrackMOS,orbitalElementsMOS</EventWorkerId>
