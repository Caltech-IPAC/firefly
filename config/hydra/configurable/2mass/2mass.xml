<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Project SYSTEM "../../project.dtd">

<Project>
    <Name>2mass</Name>
    <Title>The Two Micron All Sky Survey at IPAC</Title>

    <SearchGroup>
        <Name>2mass</Name>
        <Title>The Two Micron All Sky Survey at IPAC</Title>

        <SearchType>
            <Name>2mass_1</Name>
            <Title>Search 2MASS (Tabbed Results)</Title>
            <Tooltip>XML version of the original Demo 2MASS</Tooltip>
            <Form>
                <FieldGroup align="left">
                    <PreDefField id="targetPanel"/>
                    <DEGREE units="arcsec">
                        <Name>DemoSearch2MassPosCmd.field.radius</Name>
                        <Title>Radius</Title>
                        <Default>500</Default>
                        <Cached/>
                    </DEGREE>
                </FieldGroup>
            </Form>
            <Query id="2MassQuery" searchProcessorId="2MassQuery">
                <Param key="test1" value="VALUE1"/>
                <Param key="test2" value="VALUE2"/>
                <Metadata>
                    <Param key="CENTER_COLUMN" value="center_ra;center_dec;EQ_J2000"/>
                    <Param key="PREVIEW_COLUMN" value="download"/>
                    <Param key="RESOURCE_TYPE" value="URL"/>
                    <Param key="col.related" value="coadd_key"/>
                </Metadata>
                <Download id="2MassDownload" title="Download Options" filePrefix="2MassFiles" titlePrefix="2Mass">
                    <Param key="URL_DOWNLOAD_COLUMN" value="download"/>
                    <Param key="URL_PARAM_FILENAME" value="name"/>
                    <FieldGroup>
                        <EnumString orientation="vertical">
                            <Name>allBands</Name>
                            <Title>Download All Bands (H/J/K)</Title>
                            <Mask>[RADIO]</Mask>
                            <PreferWidth>400</PreferWidth>
                            <Default>yes</Default>
                            <EnumValue id="yes">
                                <Title>Yes</Title>
                            </EnumValue>
                            <EnumValue id="no">
                                <Title>No</Title>
                            </EnumValue>
                        </EnumString>
                    </FieldGroup>
                </Download>
            </Query>
            <Result>
                <EventWorker id="target" type="ActiveTarget">
                    <QueryId>2MassQuery</QueryId>
                </EventWorker>
                <EventWorker id="rowTarget" type="RowTarget">
                    <QueryId>2MassQuery</QueryId>
                </EventWorker>
                <EventWorker id="pers_art" type="DataSetVisQuery">
                    <QueryId>2MassQuery</QueryId>
                    <Param key="searchProcessorId" value="2MassQueryArtifact"/>
                    <Param key="Events" value="onRowHighlightChange"/>
                    <Param key="ArgCols" value="glint_art,pers_art"/>
                    <Param key="ExtraParams" value="type=pers"/>
                    <Param key="COLOR" value="orange"/>
                    <Param key="SYMBOL" value="CROSS"/>
                    <Param key="Title" value="Persistence Artifacts"/>
                </EventWorker>
                <EventWorker id="glint_art" type="DataSetVisQuery">
                    <QueryId>2MassQuery</QueryId>
                    <Param key="searchProcessorId" value="2MassQueryArtifact"/>
                    <Param key="Events" value="onRowHighlightChange"/>
                    <Param key="ExtraParams" value="type=glint"/>
                    <Param key="ArgCols" value="glint_art,pers_art"/>
                    <Param key="COLOR" value="purple"/>
                    <Param key="SYMBOL" value="DIAMOND"/>
                    <Param key="Title" value="Glint Artifacts"/>
                </EventWorker>
                <Layout>
                    <SplitPanel>
                        <East layout="tab" title="Details" initialWidth="420">
                            <Preview type="DATA_VIEW">
                                <QueryId>2MassQuery</QueryId>
                                <Title>Preview j</Title>
                                <EventWorkerId>target,rowTarget,pers_art,glint_art</EventWorkerId>
                                <Param key="ArgCols" value="download,name"/>
                                <Param key="searchProcessorId" value="TwoMassFileRetrieve"/>
                                <Param key="ExtraParams" value="band=j,other=adf"/>
                                <Param key="Title" value="2Mass j"/>
                                <Param key="Zoom" value=".5"/>
                            </Preview>
                            <Preview type="DATA_VIEW">
                                <QueryId>2MassQuery</QueryId>
                                <Title>Preview h</Title>
                                <EventWorkerId>target,rowTarget,pers_art,glint_art</EventWorkerId>
                                <Param key="ArgCols" value="download,name"/>
                                <Param key="searchProcessorId" value="TwoMassFileRetrieve"/>
                                <Param key="ExtraParams" value="band=h,other=adf"/>
                                <Param key="Title" value="2Mass h"/>
                                <Param key="Zoom" value=".5"/>
                            </Preview>
                            <Preview type="DATA_VIEW">
                                <QueryId>2MassQuery</QueryId>
                                <Title>Preview K</Title>
                                <EventWorkerId>target,rowTarget,pers_art,glint_art</EventWorkerId>
                                <Param key="ArgCols" value="download,name"/>
                                <Param key="searchProcessorId" value="TwoMassFileRetrieve"/>
                                <Param key="ExtraParams" value="band=k,other=adf"/>
                                <Param key="Title" value="2Mass K"/>
                                <Param key="Zoom" value=".5"/>
                            </Preview>
                            <Preview type="COVERAGE_VIEW">
                                <QueryId>2MassQuery</QueryId>
                                <EventWorkerId>target,rowTarget,pers_art,glint_art</EventWorkerId>
                                <Title>Coverage</Title>
                                <!-- eventually remove - moved to Query/Metadata -->
                                <Param key="PREVIEW_COLUMN" value="download"/>
                                <Param key="RESOURCE_TYPE" value="URL"/>
                            </Preview>
                        </East>
                        <Center title="Results">
                            <Table type="TABLE">
                                <QueryId>2MassQuery</QueryId>
                                <Name>aorByPos</Name>
                                <Title>AOR Table</Title>
                                <ShortDescription>Results</ShortDescription>
                                <Param key="DL_ID" value="processorIdForDownloading"/>
                                <Param key="DL_TITLE_PREFIX" value="auto"/>
                                <Param key="DL_FILE_PREFIX" value="auto"/>
                            </Table>
                        </Center>
                    </SplitPanel>
                </Layout>
            </Result>
        </SearchType>
