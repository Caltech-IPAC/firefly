<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Project SYSTEM "../../project.dtd">

<Project>
    <Name>lsst</Name>
    <Title>LSST Image Archive</Title>
    <Properties>
        <Param key="footer" value="lsst_footer.html"/>
        <Param key="smallIcon" value="lsst_logo_left.html"/>
        <Param key="helpMenu" value="Lsst Help;Lsst Help;overviewHelp"/>
        <Param key="catalogSearchMethod" value="POLYGON"/>
    </Properties>

    <Catalog name="LSST">
        <Host>https://tester:summer2012@osiris.ipac.caltech.edu</Host>
        <CatalogURL><![CDATA[/cgi-bin/Gator/nph-scan?mode=ascii]]></CatalogURL>
        <OriginalFilename>master-lsst-cat2-original.tbl</OriginalFilename>
        <MasterCatFilename>master-lsst-catalog.tbl</MasterCatFilename>
        <AddtlReqSearchParams>
            <GatorParam requestName="server" masterCatName="server"/>
            <GatorParam requestName="database" masterCatName="database"/>
            <GatorParam requestName="ddfile" masterCatName="ddname"/>
            <GatorParam requestName="xpffile" masterCatName="xpffile"/>
        </AddtlReqSearchParams>
    </Catalog>

    <SearchGroup>
        <Name>lsst</Name>
        <Title>LSST Image Archive</Title>

        <SearchType helpId="searching.byPosition">
            <Name>lsst_coadd</Name>
            <Title>Search by Position</Title>
            <Tooltip>Search LSST Image by Position</Tooltip>
            <Form>
                <FormEventWorker id="fdvController_1" type="FieldDefVisibilityControl" xid="fdvController_1">
                    <Param key="masters" value="intersect=CENTER"></Param>
                    <Param key="show" value="subsize,mcenter"></Param>
                    <Param key="hide" value="size"></Param>
                </FormEventWorker>
                <FormEventWorker id="fdvController_2" type="FieldDefVisibilityControl" xid="fdvController_2">
                    <Param key="masters" value="or(intersect=ENCLOSED,intersect=OVERLAPS)"></Param>
                    <Param key="show" value="size"></Param>
                    <Param key="hide" value="subsize,mcenter"></Param>
                </FormEventWorker>
                <FormEventWorker id="fdvController_3" type="FieldDefVisibilityControl" xid="fdvController_3">
                    <Param key="masters" value="intersect=COVERS"></Param>
                    <Param key="show" value="size,subsize,mcenter"></Param>
                </FormEventWorker>
                <FieldGroup align="left" labelWidth="300">
                    <FieldGroup type="activeTabPane" typeName="POS UPL TabPane" width="640px" height="106px">
                        <FieldGroup>
                            <Title>Single Object</Title>
                            <PreDefField id="simpleTargetPanel">
                            </PreDefField>
                        </FieldGroup>
                        <FieldGroup labelWidth="70">
                            <Title>Multi-Objects</Title>
                            <File>
                                <Name>filename</Name>
                                <Title>File Name</Title>
                                <Cached/>
                                <Size>62</Size>
                                <ShortDescription>Select a file to upload</ShortDescription>
                                <ErrorDescription>File upload error</ErrorDescription>
                            </File>
                            <Help helpId="searching.byTable">
                                <Title>Help on file format</Title>
                            </Help>
                        </FieldGroup>
                    </FieldGroup>
                    <FieldGroup align="left" labelWidth="300" xid="regionSearch">
                        <EnumString>
                            <Name>intersect</Name>
                            <Title>Search Type (Region Intersection)</Title>
                            <PreferWidth>150</PreferWidth>
                            <Default>CENTER</Default>
                            <ShortDescription>Search Type (Region Intersection)</ShortDescription>
                            <EnumValue id="CENTER">
                                <Title>Image contains target</Title>
                            </EnumValue>
                            <EnumValue id="COVERS">
                                <Title>Image covers entire search region</Title>
                            </EnumValue>
                            <EnumValue id="ENCLOSED">
                                <Title>Image is entirely enclosed by search region</Title>
                            </EnumValue>
                            <EnumValue id="OVERLAPS">
                                <Title>Any pixel overlaps search region</Title>
                            </EnumValue>
                        </EnumString>
                        <DEGREE units="arcsec">
                            <Name>size</Name>
                            <Title>Search Region (Square) Size</Title>
                            <Default>30</Default>
                            <PreferWidth>16</PreferWidth>
                            <ShortDescription>Enter the region size of the search</ShortDescription>
                            <ErrorDescription>Region size out of range</ErrorDescription>
                            <Min>1</Min>
                            <Max>43200</Max>
                            <Cached/>
                        </DEGREE>
                        <DEGREE units="arcsec">
                            <Name>subsize</Name>
                            <Title>Return Image Size (leave blank for full images)</Title>
                            <Default>100</Default>
                            <PreferWidth>16</PreferWidth>
                            <ShortDescription>Specifies the size of the desired image cutout. Omitting this constraint will result in visualization/return of entire L1 images.

    In multi-input mode, this takes effect only for input tables without a subsize column.</ShortDescription>
                            <ErrorDescription>Invalid entry!</ErrorDescription>
                            <Min>1</Min>
                            <Max>7200</Max>
                            <NullAllowed/>
                        </DEGREE>
                        <EnumString>
                            <Name>mcenter</Name>
                            <Title>Return only the most centered image containing the target:</Title>
                            <PreferWidth>150</PreferWidth>
                            <Mask>[RADIO]</Mask>
                            <Default>all</Default>
                            <NullAllowed/>
                            <ShortDescription>Specifies whether to return only the most centered (in pixel space) image-set for the given input position.

    In multi-input mode, this flag applies only for input tables without a best column.</ShortDescription>
                            <EnumValue id="all">
                                <Title>No</Title>
                            </EnumValue>
                            <EnumValue id="mcen">
                                <Title>Yes</Title>
                            </EnumValue>
                        </EnumString>
                        <FieldGroup type="activeCollapsiblePanel" labelWidth="200">
                            <Title>Optional constraints for Image Search</Title>
                            <FieldGroup align="left" labelWidth="100">
                                <EnumString orientation="horizontal">
                                    <Name>filterName</Name>
                                    <Title>Select Filter(s):</Title>
                                    <PreferWidth>225</PreferWidth>
                                    <Mask>[CHECKBOX]</Mask>
                                    <Default>_all_</Default>
                                    <ShortDescription>Telescope</ShortDescription>
                                    <ErrorDescription>Please select at least one filter</ErrorDescription>
                                    <EnumValue id="_all_">
                                        <Title>All</Title>
                                    </EnumValue>
                                    <EnumValue id="u">
                                        <Title>u</Title>
                                    </EnumValue>
                                    <EnumValue id="g">
                                        <Title>g</Title>
                                    </EnumValue>
                                    <EnumValue id="r">
                                        <Title>r</Title>
                                    </EnumValue>
                                    <EnumValue id="i">
                                        <Title>i</Title>
                                    </EnumValue>
                                    <EnumValue id="z">
                                        <Title>z</Title>
                                    </EnumValue>
                                </EnumString>
                            </FieldGroup>
                        </FieldGroup>
                    </FieldGroup>
                </FieldGroup>
            </Form>
            <Query id="lsstQuery_coadd" searchProcessorId="LsstQuery" xid="lsstQuery_coadd">
                <Param key="host" value="%lsst.ibe.host%"/>
                <Param key="schemaGroup" value="lsst"/>
                <Param key="schema" value="summer2012"/>
                <Param key="table" value="good_seeing_coadd"/>
                <Param key="ProductLevel" value="coadd"/>
                <Param key="SortInfo" value="SortInfo=ASC,patch"/>
