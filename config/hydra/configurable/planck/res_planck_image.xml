<!DOCTYPE Result SYSTEM "../../project.dtd">


<Result xid="planckimagResult">
    <EventWorker id="imagesWorker2" type="DataSetQuery" xid="imagesWorker2">
        <QueryId>
            planck30,planck44,planck70,planck100,planck143,planck217,planck353,planck545,planck857
        </QueryId>
        <Param key="searchProcessorId" value="planckCutoutQuery"/>
        <Param key="Events" value="SearchResultEnd"/>
    </EventWorker>
    <Layout>
        <SplitPanel>
            <South layout="fixedTab" initialHeight="350">

                <Preview type="BasicPreviewImageGrid">
                    <QueryId>
                        planck30,planck44,planck70,planck100,planck143,planck217,planck353,planck545,planck857
                    </QueryId>
                    <EventWorkerId>imagesWorker2</EventWorkerId>
                    <Title>Cutouts</Title>
                    <Param key="Readout"
                           value="0=TITLE,2=IMAGE_PIXEL,1=GALACTIC,3=Pixel Size"/>
                    <Param key="LockRelated" value="true"/>
                    <Param key="UseScrollBars" value="false"/>
                    <Param key="TitleDesc.PLANCK"
                           value="(Planck Cutouts)"/>
                    <Param key="TitleDesc.WMAP"
                           value="(WMAP Cutouts)"/>
                    <Param key="TitleDesc.IRAS"
                           value="(IRAS Cutouts from ISSA plates)"/>
                </Preview>
            </South>
            <East layout="fixedTab" initialWidth="200">
                <Preview type="RowDetails">
                    <Title>Notes/Details</Title>
                    <Param key="NOTES_PROC_ID" value="planckNotesQuery"/>
                </Preview>
            </East>
            <Center layout="tab" tagIt='true' helpId="results">
                <Table type="TABLE">
                    <QueryId>planck30</QueryId>
                    <Title>30 GHz</Title>
                    <ShortDescription>30 GHz</ShortDescription>
                </Table>
                <Table type="TABLE">
                    <QueryId>planck44</QueryId>
                    <Title>44 GHz</Title>
                    <ShortDescription>44 GHz</ShortDescription>
                </Table>
                <Table type="TABLE">
                    <QueryId>planck70</QueryId>
                    <Title>70 GHz</Title>
                    <ShortDescription>70 GHz</ShortDescription>
                </Table>
                <Table type="TABLE">
                    <QueryId>planck100</QueryId>
                    <Title>100 GHz</Title>
                    <ShortDescription>100 GHz</ShortDescription>
                </Table>
                <Table type="TABLE">
                    <QueryId>planck143</QueryId>
                    <Title>143 GHz</Title>
                    <ShortDescription>143 GHz</ShortDescription>
                </Table>
                <Table type="TABLE">
                    <QueryId>planck217</QueryId>
                    <Title>217 GHz</Title>
                    <ShortDescription>217 GHz</ShortDescription>
                </Table>
                <Table type="TABLE">
                    <QueryId>planck353</QueryId>
                    <Title>353 GHz</Title>
                    <ShortDescription>353 GHz</ShortDescription>
                </Table>
                <Table type="TABLE">
                    <QueryId>planck545</QueryId>
                    <Title>545 GHz</Title>
                    <ShortDescription>545 GHz</ShortDescription>
                </Table>
                <Table type="TABLE">
                    <QueryId>planck857</QueryId>
                    <Title>857 GHz</Title>
                    <ShortDescription>857 GHz</ShortDescription>
                </Table>
            </Center>
        </SplitPanel>
    </Layout>
</Result>

