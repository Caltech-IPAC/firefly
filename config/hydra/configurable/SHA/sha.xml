<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Project SYSTEM "../../project.dtd">

<Project>
    <Name>sha</Name>
    <Title>Spitzer Heritage Archive</Title>

    <SearchGroup>
        <Name>sha</Name>
        <Title>Spitzer Heritage Archive</Title>

        <SearchType>
            <Name>SearchByPosition</Name>
            <Title>Position</Title>
            <Tooltip>Position Search</Tooltip>
            <Form>
                <FieldGroup align="left">
                    <FieldGroup type="tabPane" width="540px" height="135px">
                        <FieldGroup>
                            <Title>Position</Title>
                            <PreDefField id="targetPanel"/>
                        </FieldGroup>
                        <FieldGroup>
                            <Title>From File</Title>
                            <File>
                                <Name>file</Name>
                                <Title>Table/File Upload</Title>
                                <Cached/>
                                <ShortDescription>Select a file to upload</ShortDescription>
                                <ErrorDescription>File upload error</ErrorDescription>
                            </File>
                        </FieldGroup>
                    </FieldGroup>
                    <DEGREE units="arcsec">
                        <Name>radius</Name>
                        <Title>Radius</Title>
                        <ShortDescription>Enter the radius of the search</ShortDescription>
                        <ErrorDescription>Radius out of range</ErrorDescription>
                        <PreferWidth>10</PreferWidth>
                        <MaxWidth>10</MaxWidth>
                    </DEGREE>
                    <EnumString>
                        <Name>matchByAOR</Name>
                        <Title>Apply spatial constraints to:</Title>
                        <PreferWidth>300</PreferWidth>
                        <Mask>[RADIO]</Mask>
                        <Default>no</Default>
                        <ShortDescription>Should we display ALL products associated with matching AORs or only those
                            products that are within spatial constraints?
                        </ShortDescription>
                        <EnumValue id="yes">
                            <Title>AORs</Title>
                        </EnumValue>
                        <EnumValue id="no">
                            <Title>Individual data products</Title>
                        </EnumValue>
                    </EnumString>
                </FieldGroup>
                <FieldGroup xid="prodtype">
                    <EnumString>
                        <Name>prodtype</Name>
                        <Title>Display search results in tabs for:</Title>
                        <PreferWidth>300</PreferWidth>
                        <Mask>[CHECKBOX]</Mask>
                        <Default>aor,pbcd</Default>
                        <ShortDescription>Select at least one product, you'd like to see in results</ShortDescription>
                        <ErrorDescription>At least one product type must be checked</ErrorDescription>
                        <EnumValue id="aor">
                            <Title>Observation Request (AOR)</Title>
                        </EnumValue>
                        <EnumValue id="bcd">
                            <Title>Level 1 (BCD)</Title>
                        </EnumValue>
                        <EnumValue id="pbcd">
                            <Title>Level 2 (PBCD)</Title>
                        </EnumValue>
                    </EnumString>
                </FieldGroup>
            </Form>
            <Query id="SearchBCD" searchProcessorId="SearchBCD">
                <Metadata>
                    <Param key="CENTER_COLUMN" value="center_ra;center_dec;EQ_J2000"/>
                    <Param key="PREVIEW_COLUMN" value="download"/>
                    <Param key="RESOURCE_TYPE" value="URL"/>
                </Metadata>
            </Query>
            <Result>
                <Layout>
                    <SplitPanel>
                        <East layout="horizontal">
                            <Preview type="COVERAGE_VIEW">
                                <QueryId>SearchBCD</QueryId>
                                <!-- eventually remove - moved to Query/Metadata -->
                                <Param key="PREVIEW_COLUMN" value="download"/>
                                <Param key="RESOURCE_TYPE" value="URL"/>
                            </Preview>
                        </East>
                        <Center layout="horizontal">
                            <Table type="TABLE">
                                <QueryId>SearchBCD</QueryId>
                                <Name>BCDTable</Name>
                                <Title>BCD Table</Title>
                                <ShortDescription>BCD Short Description</ShortDescription>
                                <Param key="DL_ID" value="processorIdForDownloading"/>
                                <Param key="DL_TITLE_PREFIX" value="auto"/>
                                <Param key="DL_FILE_PREFIX" value="auto"/>
                            </Table>
                        </Center>
                    </SplitPanel>
                </Layout>
            </Result>
        </SearchType>
        <SearchType>
            <Name>AbstractSearch</Name>
            <Title>Abstract Text</Title>
            <Tooltip>Full text search of program's abstract</Tooltip>
            <Form>
                <FieldGroup>
                    <String>
                        <Name>search</Name>
                        <Title>Search</Title>
                        <PreferWidth>30</PreferWidth>
                        <ShortDescription>Enter terms to search on</ShortDescription>
                        <ErrorDescription>Must not be blank</ErrorDescription>
                    </String>
                    <Label><![CDATA[<i>Enter the string on which you'd like to search.</i>]]></Label>
                </FieldGroup>
            </Form>
            <Query id="SearchBCD" searchProcessorId="SearchBCD">
                <Metadata>
                    <Param key="CENTER_COLUMN" value="center_ra;center_dec;EQ_J2000"/>
                    <Param key="PREVIEW_COLUMN" value="download"/>
                    <Param key="RESOURCE_TYPE" value="URL"/>
                </Metadata>
            </Query>
            <Result>
                <Layout>
                    <SplitPanel>
                        <East layout="horizontal">
                            <Preview type="COVERAGE_VIEW">
                                <QueryId>SearchBCD</QueryId>
                                <!-- eventually remove - moved to Query/Metadata -->
                                <Param key="PREVIEW_COLUMN" value="download"/>
                                <Param key="RESOURCE_TYPE" value="URL"/>
                            </Preview>
                        </East>
                        <Center layout="horizontal">
                            <Table type="TABLE">
                                <QueryId>SearchBCD</QueryId>
                                <Name>BCDTable</Name>
                                <Title>BCD Table</Title>
                                <ShortDescription>BCD Short Description</ShortDescription>
                                <Param key="DL_ID" value="processorIdForDownloading"/>
                                <Param key="DL_TITLE_PREFIX" value="auto"/>
                                <Param key="DL_FILE_PREFIX" value="auto"/>
                            </Table>
                        </Center>
                    </SplitPanel>
                </Layout>
            </Result>
        </SearchType>
        <SearchType>
            <Name>SearchByNaifID</Name>
            <Title>Moving Object</Title>
            <Tooltip>Moving Object Search</Tooltip>
            <Form>
                <FieldGroup align="left">
                    <FieldGroup align="right" labelWidth="150">
                        <String>
                            <Name>tgtName</Name>
                            <Title>Moving Target Name</Title>
                            <PreferWidth>20</PreferWidth>
                            <ShortDescription>Moving target name</ShortDescription>
                            <ErrorDescription>Unable to find NAIF IDs for this moving target</ErrorDescription>
                        </String>
                        <String>
                            <Name>naifID</Name>
                            <Title>NAIF ID</Title>
                            <PreferWidth>20</PreferWidth>
                            <ShortDescription>NAIF ID</ShortDescription>
                            <ErrorDescription>Must be numerical value. Example 2076019,3079505</ErrorDescription>
                        </String>
                        <Label>
                            <![CDATA[<i>Enter moving target name (e.g. Gaspra); alternatively, enter NAIF ID as one or more comma delimited integers e.g. 2000021,2003226.</i>]]></Label>
                        <FieldGroup ref-xid="prodtype"/>
                    </FieldGroup>
                </FieldGroup>
            </Form>
            <Query id="SearchBCD" searchProcessorId="SearchBCD">
                <Metadata>
                    <Param key="CENTER_COLUMN" value="center_ra;center_dec;EQ_J2000"/>
                    <Param key="PREVIEW_COLUMN" value="download"/>
                    <Param key="RESOURCE_TYPE" value="URL"/>
                </Metadata>
            </Query>
            <Result>
                <Layout>
                    <SplitPanel>
                        <East layout="horizontal">
                            <Preview type="COVERAGE_VIEW">
                                <QueryId>SearchBCD</QueryId>
                                <!-- eventually remove - moved to Query/Metadata -->
                                <Param key="PREVIEW_COLUMN" value="download"/>
                                <Param key="RESOURCE_TYPE" value="URL"/>
                            </Preview>
                        </East>
                        <Center layout="horizontal">
                            <Table type="TABLE">
                                <QueryId>SearchBCD</QueryId>
                                <Name>BCDTable</Name>
                                <Title>BCD Table</Title>
                                <ShortDescription>BCD Short Description</ShortDescription>
                                <Param key="DL_ID" value="processorIdForDownloading"/>
                                <Param key="DL_TITLE_PREFIX" value="auto"/>
                                <Param key="DL_FILE_PREFIX" value="auto"/>
                            </Table>
                        </Center>
                    </SplitPanel>
                </Layout>
            </Result>
        </SearchType>
        <SearchType>
            <Name>SearchByDate</Name>
            <Title>Observation Date</Title>
            <Tooltip>Observation Date Search</Tooltip>
            <Form>
                <FieldGroup align="left">
                    <FieldGroup type="datePanel"/>
                    <FieldGroup ref-xid="prodtype"/>
                    <FieldGroup type="collapsiblePanel">
                        <Title>More options</Title>
                        <EnumString orientation="vertical">
                            <Name>irac</Name>
                            <Title>IRAC Options</Title>
                            <PreferWidth>225</PreferWidth>
                            <Mask>[CHECKBOX]</Mask>
                            <Default>_all_</Default>
                            <ShortDescription>Select IRAC Observing Mode and Wavelength</ShortDescription>
                            <ErrorDescription>At least one product type must be checked</ErrorDescription>
                            <EnumValue id="map">
                                <Title>IRAC Mapping</Title>
                            </EnumValue>
                            <EnumValue id="post">
                                <Title>IRAC Post Cryo</Title>
                            </EnumValue>
                            <EnumValue id="w3">
                                <Title><![CDATA[3.6 &mu;m]]></Title>
                            </EnumValue>
                            <EnumValue id="w4">
                                <Title><![CDATA[4.5 &mu;m]]></Title>
                            </EnumValue>
                            <EnumValue id="w5">
                                <Title><![CDATA[5.8 &mu;m]]></Title>
                            </EnumValue>
                            <EnumValue id="w8">
                                <Title><![CDATA[8.0 &mu;m]]></Title>
                            </EnumValue>
                        </EnumString>
                    </FieldGroup>
                </FieldGroup>
            </Form>
            <Query id="SearchBCD" searchProcessorId="SearchBCD">
                <Metadata>
                    <Param key="CENTER_COLUMN" value="center_ra;center_dec;EQ_J2000"/>
                    <Param key="PREVIEW_COLUMN" value="download"/>
                    <Param key="RESOURCE_TYPE" value="URL"/>
                </Metadata>
            </Query>
            <Result>
                <Layout>
                    <SplitPanel>
                        <East layout="horizontal">
                            <Preview type="COVERAGE_VIEW">
                                <QueryId>SearchBCD</QueryId>
                                <!-- eventually remove - moved to Query/Metadata -->
                                <Param key="PREVIEW_COLUMN" value="download"/>
                                <Param key="RESOURCE_TYPE" value="URL"/>
                            </Preview>
                        </East>
                        <Center layout="horizontal">
                            <Table type="TABLE">
                                <QueryId>SearchBCD</QueryId>
                                <Name>BCDTable</Name>
                                <Title>BCD Table</Title>
                                <ShortDescription>BCD Short Description</ShortDescription>
                                <Param key="DL_ID" value="processorIdForDownloading"/>
                                <Param key="DL_TITLE_PREFIX" value="auto"/>
                                <Param key="DL_FILE_PREFIX" value="auto"/>
                            </Table>
                        </Center>
                    </SplitPanel>
                </Layout>
            </Result>
        </SearchType>
        <SearchType>
            <Name>SearchByRequestID</Name>
            <Title>AORKEY</Title>
            <Tooltip>AORKEY Search</Tooltip>
            <Form>
                <FieldGroup align="left">
                    <String>
                        <Name>requestID</Name>
                        <Title>AORKEY</Title>
                        <PreferWidth>30</PreferWidth>
                        <ShortDescription>AORKEY, also known as Request ID</ShortDescription>
                        <ErrorDescription>Need to be numerical value. Example 13628416,13629696</ErrorDescription>
                    </String>
                    <Label><![CDATA[<i>Enter one or more values delimited by a ',' e.g. 21641216,17758208.</i>]]></Label>
                </FieldGroup>
                <FieldGroup ref-xid="prodtype"/>
            </Form>
            <Query id="SearchBCD" searchProcessorId="SearchBCD">
                <Metadata>
                    <Param key="CENTER_COLUMN" value="center_ra;center_dec;EQ_J2000"/>
                    <Param key="PREVIEW_COLUMN" value="download"/>
                    <Param key="RESOURCE_TYPE" value="URL"/>
                </Metadata>
            </Query>
            <Result>
                <Layout>
                    <SplitPanel>
                        <East layout="horizontal">
                            <Preview type="COVERAGE_VIEW">
                                <QueryId>SearchBCD</QueryId>
                                <!-- eventually remove - moved to Query/Metadata -->
                                <Param key="PREVIEW_COLUMN" value="download"/>
                                <Param key="RESOURCE_TYPE" value="URL"/>
                            </Preview>
                        </East>
                        <Center layout="horizontal">
                            <Table type="TABLE">
                                <QueryId>SearchBCD</QueryId>
                                <Name>BCDTable</Name>
                                <Title>BCD Table</Title>
                                <ShortDescription>BCD Short Description</ShortDescription>
                                <Param key="DL_ID" value="processorIdForDownloading"/>
                                <Param key="DL_TITLE_PREFIX" value="auto"/>
                                <Param key="DL_FILE_PREFIX" value="auto"/>
                            </Table>
                        </Center>
                    </SplitPanel>
                </Layout>
            </Result>
        </SearchType>
    </SearchGroup>

</Project>
