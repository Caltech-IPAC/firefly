                    </EnumValue>
                    <EnumValue id="2">
                        <Title>W2</Title>
                    </EnumValue>
                    <EnumValue id="3">
                        <Title>W3</Title>
                    </EnumValue>
                    <EnumValue id="4">
                        <Title>W4</Title>
                    </EnumValue>
                </EnumString>
            </FieldGroup>
