<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Mission SYSTEM "Mission.dtd">

<Mission name="2mass" dataTypes="ImageSet,Catalog">
    <Title>2MASS</Title>
    <SpacialTypes catalog="Cone,Box" imageSet="Cone,Box"/>
    <Desc>The Two Micron All Sky Survey at IPAC</Desc>


    <ImageSet name="2MassAtAsky">
        <Title>2MASS All Sky (Atlas)</Title>
        <Desc>2MASS All Sky Release Survey Images (Atlas)</Desc>
        <DataSource searchProcId="2MassQuery">
            <Param key="type" value="at"/>
            <Param key="ds" value="asky"/>
        </DataSource>
        <xi:include href="2mass/2mass_optional.xml"/>
    </ImageSet>
    <ImageSet name="2MassAtAskyw">
        <Title>2MASS Full Survey (Atlas)</Title>
        <Desc>2MASS 2MASS Full Survey Images (Atlas)</Desc>
        <DataSource searchProcId="2MassQuery">
            <Param key="type" value="at"/>
            <Param key="ds" value="askyw"/>
        </DataSource>
        <xi:include href="2mass/2mass_optional.xml"/>
    </ImageSet>
    <ImageSet name="2MassAtSx">
        <Title>2MASS 6X Catalog Images (Atlas)</Title>
        <Desc>2MASS 6X Catalog Images (Atlas)</Desc>
        <DataSource searchProcId="2MassQuery">
            <Param key="type" value="at"/>
            <Param key="ds" value="sx"/>
        </DataSource>
        <xi:include href="2mass/2mass_optional.xml"/>
    </ImageSet>
    <ImageSet name="2MassAtSxw">
        <Title>2MASS Full 6X Images (Atlas)</Title>
        <Desc>2MASS Full 6X Images (Atlas)</Desc>
        <DataSource searchProcId="2MassQuery">
            <Param key="type" value="at"/>
            <Param key="ds" value="sxw"/>
        </DataSource>
        <xi:include href="2mass/2mass_optional.xml"/>
    </ImageSet>
    <ImageSet name="2MassAtCal">
        <Title>2MASS Calibration Images (Atlas)</Title>
        <Desc>2MASS Calibration Images (Atlas)</Desc>
        <DataSource searchProcId="2MassQuery">
            <Param key="type" value="at"/>
            <Param key="ds" value="cal"/>
        </DataSource>
        <xi:include href="2mass/2mass_optional.xml"/>
    </ImageSet>


</Mission>
