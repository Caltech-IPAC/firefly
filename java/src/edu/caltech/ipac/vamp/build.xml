<project name="Vamp build File" default="all" basedir=".">
    <description>
        The build file for building Vamp
    </description>
<!-- Project level init file -->
    <property name="build.root" value="${basedir}/../../../../.."/>
    <import file="${build.root}/tools/init.xml"/>

<!-- local properties -->
    <property name="launch.page" value="Vamp.html"/>
    <property name="gwt.module" value="edu.caltech.ipac.vamp.Vamp"/>
    <property name="gwt.module.conf.file" value="Vamp.gwt.xml"/>

    <import file="${build.root}/../config/vamp/build.xml"/>
    <import file="${build.root}/tools/webapp_build_template.xml"/>

</project>
