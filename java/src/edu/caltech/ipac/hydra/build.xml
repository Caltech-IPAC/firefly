<project name="Hydra build File" default="hydra" basedir=".">
    <description>
        The build file for building Hydra
    </description>
<!-- Project level init file -->
    <property name="build.root" value="${basedir}/../../../../.."/>

<!-- local properties -->
    <property name="launch.page" value="Hydra.html"/>
    <property name="gwt.module" value="edu.caltech.ipac.hydra.Hydra"/>
    <property name="gwt.module.conf.file" value="Hydra.gwt.xml"/>
    <property name="app-src-dir" value="hydra"/>
    <property name="proj-title" value="IRSA"/>

    <target name="hydra">
        <ant antfile="build_hydra.xml"/>
    </target>
</project>
