<module rename-to="fftools">
    <inherits name='edu.caltech.ipac.firefly.FireFly'/>
    <inherits name='edu.caltech.ipac.theme.standard.Standard'/>

    <!-- Specify the app entry point class.                   -->
    <entry-point class='edu.caltech.ipac.fftools.core.FireflyToolsEntryPoint'/>

    <set-property name="gwt.suppressNonStaticFinalFieldWarnings" value="true" />
    <source path="core"/>

    <add-linker name="xsiframe" />
    <set-configuration-property name="devModeRedirectEnabled" value="true"/>
    <set-property name="compiler.useSourceMaps" value="true" />
    <!--<set-property name="gwt.codeserver.port" value="9876" />-->


</module>
