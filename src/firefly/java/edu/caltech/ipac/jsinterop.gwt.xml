<module rename-to="jsinterop">

    <!-- Specify the app entry point class.                   -->
    <entry-point class='edu.caltech.ipac.firefly.core.FireflyEntryPoint'/>

    <source path="firefly/core">
        <include name="FireflyEntryPoint.java"/>
    </source>

    <source path="firefly/visualize/" >
        <include name="ProjectionSerializer.java"/>
        <include name="ProjectionParams.java"/>
        <include name="package-info.java"/>
    </source>

    <source path="astro/conv"/>

    <source path="visualize/plot/projection"/>
    <source path="visualize/plot/" >
        <include name="ProjectionPt.java"/>
        <include name="*Pt.java"/>
        <include name="CoordinateSys.java"/>
        <include name="ProjectionException.java"/>

        <exclude name="ResolvedWorldPt.java"/>
        <exclude name="ScreenPt.java"/>
    </source>

  </module>
