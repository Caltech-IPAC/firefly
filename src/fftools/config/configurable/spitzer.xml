<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Mission SYSTEM "Mission.dtd">

<Mission name="spitzer" dataTypes="ImageSet,Catalog">
    <Title>SPITZER</Title>
    <SpacialTypes catalog="Cone,Box" imageSet="Cone"/>
    <Desc>Spitzer Space Telescope</Desc>

    <ImageSet name="SEIP">
        <Title>SEIP Super Mosaics</Title>
        <Desc>Spitzer Enhances Imaging Products: Super Mosaics</Desc>
        <DataSource searchProcId="RTreeInventory">
            <Param key="serviceurl" value="https://irsa.ipac.caltech.edu/cgi-bin/SSCDemo/nph-sscdemo"/>
            <Param key="dataset" value="ivo://irsa.ipac/spitzer.enhancedImages"/>
        </DataSource>
        <Form>
            <FieldGroup align="left">
                <Label>
                    Spitzer Enhanced Imaging Products (SEIP) include data from the four channels of IRAC (3.6, 4.5, 5.8, 8 microns) and the 24 micron channel of MIPS.
                </Label>
            </FieldGroup>
        </Form>
    </ImageSet>
</Mission>
