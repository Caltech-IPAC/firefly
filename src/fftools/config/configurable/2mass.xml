<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Mission SYSTEM "Mission.dtd">

<Mission name="2mass" dataTypes="ImageSet,Catalog">
    <Title>2MASS</Title>
    <SpacialTypes catalog="Cone,Box" imageSet="Cone"/>
    <Desc>The Two Micron All Sky Survey at IPAC</Desc>

    <ImageSet name="2mass-asky">
        <Title>2MASS All Sky</Title>
        <Desc>2MASS All Sky Release Survey Images</Desc>
        <DataSource searchProcId="ibe_processor">
            <Param key="mission" value="twomass"/>
            <Param key="ds" value="asky"/>
        </DataSource>
        <xi:include href="2mass/2mass_optional.xml"/>
    </ImageSet>
    <ImageSet name="2mass-askyw">
        <Title>2MASS Full Survey</Title>
        <Desc>2MASS 2MASS Full Survey Images</Desc>
        <DataSource searchProcId="ibe_processor">
            <Param key="mission" value="twomass"/>
            <Param key="ds" value="askyw"/>
        </DataSource>
        <xi:include href="2mass/2mass_optional.xml"/>
    </ImageSet>
    <ImageSet name="2mass-sx">
        <Title>2MASS 6X Catalog Images</Title>
        <Desc>2MASS 6X Catalog Images</Desc>
        <DataSource searchProcId="ibe_processor">
            <Param key="mission" value="twomass"/>
            <Param key="ds" value="sx"/>
        </DataSource>
        <xi:include href="2mass/2mass_optional.xml"/>
    </ImageSet>
    <ImageSet name="2mass-sxw">
        <Title>2MASS Full 6X Images</Title>
        <Desc>2MASS Full 6X Images</Desc>
        <DataSource searchProcId="ibe_processor">
            <Param key="mission" value="twomass"/>
            <Param key="ds" value="sxw"/>
        </DataSource>
        <xi:include href="2mass/2mass_optional.xml"/>
    </ImageSet>
    <ImageSet name="2mass-cal">
        <Title>2MASS Calibration Images</Title>
        <Desc>2MASS Calibration Images</Desc>
        <DataSource searchProcId="ibe_processor">
            <Param key="mission" value="twomass"/>
            <Param key="ds" value="cal"/>
        </DataSource>
        <xi:include href="2mass/2mass_optional.xml"/>
    </ImageSet>


</Mission>
